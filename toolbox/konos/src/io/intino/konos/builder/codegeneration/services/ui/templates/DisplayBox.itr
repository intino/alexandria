rule type(box, decorated) and trigger(extension)
	B extends Box

rule type(box) and trigger(extension)

rule type(box, decorated) and trigger(extensionTagged)
	<B extends Box>

rule type(box) and trigger(extensionTagged)

rule type(box, decorated) and trigger(type)
	B

rule type(box, accessible) and trigger(type)
	Box

rule type(box) and trigger(type)
	$box+firstUpperCase~Box
