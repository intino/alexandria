def type(view & display)
	new io.intino.alexandria.ui.model.View()
				.container(new io.intino.alexandria.ui.model.view.container.DisplayContainer()
						 .displayLoader((target, loadingListener, instantListener, session) -> $owner+FirstUpperCase.Views.$name+FirstUpperCase.display(box, target, loadingListener, instantListener, session))
						 [.hideNavigator($hideNavigator)]
						 [.scopeManager($catalogScope)])
				.layout(View.Layout.$layout)[.width($width)].name("$name")[.label("$label")][$hidden+call]
end

