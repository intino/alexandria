def trigger(catalogDisplayLoader)
	new io.intino.alexandria.ui.model.view.container.CatalogContainer.Loader() {
		public io.intino.alexandria.ui.displays.AlexandriaAbstractCatalog load(io.intino.alexandria.ui.model.Catalog catalog, io.intino.alexandria.ui.services.push.UISession session) {
			return (io.intino.alexandria.ui.displays.AlexandriaAbstractCatalog) $package.displays.Displays.displayFor(box, catalog);
		}
	}
end

def type(viewDisplayLoaders)
	.displayLoader((element, object) -> $package.displays.Displays.displayFor(box, element))
	.displayTypeLoader((element, object) -> $package.displays.Displays.displayTypeFor(box, element))
end

def type(viewContainerSetItems)
	$item...[$NL]
end

def type(group)
	.add(new io.intino.alexandria.ui.model.view.set.item.Group()[.label("$label")].mode(Group.Mode.$mode)[.hidden($hidden)]
			[$item+group...[$NL]].name("$name"));
end

def type(items)
	.add(new io.intino.alexandria.ui.model.view.set.item.Items()
		.source((session) -> $layout+FirstUpperCase.Views.$path+FirstUppercase.items(box, session))
		.label((element, object) -> $layout+FirstUpperCase.Views.$path+FirstUppercase.label(box, element, ($modelClass) object))
		[.icon((element, object) -> $layout+FirstUpperCase.Views.$path+FirstUppercase.icon(box, element, ($modelClass) object))]
		[.bubble((element, object) -> $layout+FirstUpperCase.Views.$path+FirstUppercase.bubble(box, element, ($modelClass) object))]
		.view($view)[.hidden($hidden)])
end

def	type(item)
	.add(new io.intino.alexandria.ui.model.view.set.item.Item().label("$label").view($view)[.hidden($hidden)].name("$name"))
end

def type(hidden) trigger(call)
	.hidden((object, session) -> $owner+FirstUpperCase.Views.$view+FirstUpperCase.hidden(box, object, session))
end

def type(filter) trigger(call)
	(catalog, context, target, object, session) -> $owner+FirstUpperCase.Views.$view+firstUpperCase.filter(box, catalog, context, target, ($itemClass) object, session)
end

def type(catalogScope)
	(display, scope) -> $owner+FirstUpperCase.Views.$name+FirstUpperCase.scope(box, display, scope)
end

def trigger(zoom)
	zoom(new io.intino.alexandria.ui.model.view.container.MapContainer.Zoom()[.defaultZoom($default)][.max($max)][.min($min)])
end

def trigger(quoted)
	"$this"
end