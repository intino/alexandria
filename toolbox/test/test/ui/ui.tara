dsl Konos

Service Platform-Web as UI(title = "", favicon = "", port = "{port}")
	Use("aaa", "http://aaa.es")
	Use("bbb", "http://bbb.es")
	Use("ccc", "{siteUrl}")
	Resource("/") homePage as DisplayPage(analytics) Root
	Resource("/user") userHomePage as DisplayPage(analyze) Root Confidential
	Resource("/analytics/:sample") analyticsPage as DisplayPage(analytics)
	Resource("/error") errorPage as DisplayPage(error)

Display analytics
	displays = olap dashboard pivotTable

	Notification info as Object(PlatformInfo)
	Notification boardList as Object(BoardList)
	Notification user as Object(UserInfo)
	Notification userLoggedOut
	Notification(Client) loading
	Notification(Client) loaded

	Request logout

Display olap
	Notification timeline as Object(Timeline) List
	Notification timeRange as Object(Range)
	Notification scales as Object(ScaleList)
	Notification enableDrill
	Notification disableDrill
	Notification enableFilter
	Notification disableFilter
	Notification filtering
	Notification notFiltering
	Notification drilling
	Notification notDrilling
	Notification filterList as Object(FilterList)
	Notification ticketList as Object(NameList)
	Notification selectedTicketList as Object(NameList)

	Request addTicket as Text
	Request removeTicket as Text
	Request updateScale as Text
	Request updateRange as Object(RequestRange) List
	Request(Asset) export as Object(RequestRange)
	Request toggleDrill
	Request toggleFilter
	Request removeFilter as Text
	Request asdasd as Date

Display drillwiseHome
	displays = analytics

	Notification selectPage as Text

	Request showHome //> RegisterPath(page=home)
	Request showCategories //> RegisterPath(page=PlatformWeb.analyticsPage)
	Request showSuppliers //> RegisterPath(page=suppliersPage)
	Request showPricing //> RegisterPath(page=pricingPage)
	Request showApi as Addressable(Platform-Web.analyticsPage)
	Request showPage as Text
	Request search as Text Addressable(Platform-Web.analyticsPage)

Display timeSeries
	Notification zoomRange as Object(Range)
	Notification timeRange as Object(Range)
	Notification timeline as Object(Timeline)
	Notification formats as Object(FormatterList)
	Notification updateScale as Object(Scale)
	Notification(Client) loading
	Notification(Client) loaded
	Notification clear
	Notification eventListCount as Real
	Notification eventList as Object(EventList)
	Notification addSeries as Object(SerieList)
	Notification removeSeries as Object(NameList)
	Notification addPoints as Object(Points)

	Request updateRange as Object(RequestRange)
	Request updateRangeAndScale as Object(RequestRange)
	Request move as Object(RequestRange)

Display analyze
	displays = categorization
	Notification categorizationList as Object(CategorizationViewList)
	Notification hide
	Notification show

Display categorization
	Notification categorization as Object(CategorizationView)
	Notification enableSelection
	Notification disableSelection
	Notification(Client) loading
	Notification(Client) loaded
	Notification refreshHistogram as Object(Histogram)
	Notification refreshSelected as Object(CategoryViewList)

	Request toggle as Object(RequestCategory)
	Request refresh

Display dashboard as Accessible("oooo")
	Request open
	Request close as Object(ScaleList)

Display pivotTable
Display error


Schema ScaleList
	Attribute scale as Object(Scale) List

Schema Range
	Attribute min as Real
	Attribute max as Real


Schema PlatformInfo
	Attribute title as Text
	Attribute subtitle as Text
	Attribute logo as Text

Schema BoardList
	Schema Board
		Attribute label as Text

Schema UserInfo
	Attribute fullName as Text
	Attribute photo as Text

Schema Timeline
	Attribute min as Real
	Attribute max as Real

Schema Alert
    Attribute active as Bool
    Attribute mailingList as Text List

Schema Scale
	Attribute name as LongInteger
	Attribute label as Text
	Attribute symbol as Text


Schema RequestRange
	Attribute from as Real
	Attribute to as Real

Schema FilterList
	Schema Filter
		Schema(multiple=false) Categorization
			Attribute name as Text
			Attribute label as Text
		Schema Category
			Attribute name as Text
			Attribute label as Text

Schema CategorizationViewList
	Attribute categorizationView as Object(CategorizationView) List

Schema CategorizationView
	Attribute name as Text
	Attribute label as Text

	Attribute categorizationView as Object(CategorizationView) List
	Attribute subCategorization as Object(CategorizationView)


Schema CategoryViewList
	Attribute categoryView as Object(CategoryView) List

Schema CategoryView
	Attribute name as Text
	Attribute label as Text
	Attribute categoryView as Object(CategoryView) List

Schema FormatterList
	Schema Formatter
		Attribute name as Text
		Attribute format as Text

Schema EventList
	Schema Event
		Attribute label as Text
		Attribute Instant as Real
		Attribute render as Text
		Attribute icon as Text

Schema SerieList
	Attribute serie as Object(Serie) List

Schema Serie
	Attribute label as Text
	Attribute group as Text
	Attribute metric as Text
	Attribute color as Text
	Attribute axisColor as Text
	Attribute style as Text
	Attribute min as Integer
	Attribute max as Integer

	Attribute decimalPlaces as Object(DecimalPlaces)

	Schema data
		Attribute Instant as Real
		Attribute value as Real

Schema DecimalPlaces
	Attribute absolute as Integer
	Attribute percentage as Integer

Schema Points
	Attribute series as Object(SerieList)
	Attribute series2 as Object(SerieList)
	Attribute scale as Object(Scale)


Schema NameList
	Attribute names as Text List

Schema Histogram
	Attribute categoryView as Object(CategoryView) List
	Attribute ticket as Object(Ticket) List


Schema Ticket
	Attribute label as Text
	Attribute values as Real List
	Attribute valuesInt as Integer List

	Schema Indicator
		Attribute label as Text
		Attribute color as Text
		Attribute values as Real List

		Attribute decimalPlaces as Object(DecimalPlaces)

Schema RequestCategory
	Attribute name as Text
	Attribute categorization as Text
