Catalog(modelClass = "io.intino.octana.graph.Persona", label = "población") octanaPoblacion
	Events > OnClickRecord > OpenPanel(panel = octanaPanelPersona)
	Views
		ListView("listado", mold = octanaMoldePersona) > noRecordMessage = "no hay personas registradas"
		MapView("mapa", mold = octanaMoldePersonaUbicacion) > noRecordMessage = "no hay personas con cercas registradas"
