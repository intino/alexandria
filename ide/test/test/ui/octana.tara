Panel octanaPanelPersona
	Views
		View("resumen", layout=LeftFixed) > RenderMold(octanaMoldePersonaResumen)
		View("ficha") > RenderMold(octanaMoldePersonaCompleto)
		View("causas") causas > hidden = HiddenEnabled; RenderCatalogs(octanaCausas) > filtered = true
		View("condenas") condenas > hidden = HiddenEnabled; RenderCatalogs(octanaCondenas) > filtered = true
		View("histórico") historico > RenderCatalogs(octanaHistorico) > filtered = true
		View("tracking") tracking > hidden = HiddenEnabled; RenderDisplay(display="octanaTracking")

