dsl Konos

Client(clientID = "amidas", domain = "amidas.v4") as Ness(url = "{nessUrl}", user = "{nessUser}", password = "{nessPassword}")
	Mounter(schema=IdentityRegister)
	Mounter(schema=IdentityDelete)
	Mounter(schema=IdentityApprove)
	Mounter(schema=IdentityReject)
	Mounter(schema=IdentitySave)
	Mounter(schema=IdentityEnableManager)
	Mounter(schema=IdentityDisableManager)


	Mounter(schema=CapabilityAssign)
	Mounter(schema=CapabilityRelease)

	Mounter(schema=WorkerGroupAssign)
	Mounter(schema=WorkerGroupRelease)

	Mounter(schema=AuthorizationRegister)
	Mounter(schema=AuthorizationSave)
	Mounter(schema=AuthorizationDelete)
	Feeder(dayAheadReport)
	Feeder(intradayReport)
	Feeder(eexMasterDataUnit)
	Feeder(actualGeneration)

	Mounter(schema = empty) dialog
	Mounter(schema = empty) survey
	Mounter(schema = empty) revocation
	Mounter(schema = AddGoal) addgoal
	Mounter(schema = RemoveGoal) removegoal



