dsl Proteo

use DataTypes

Facet Ness on Client
	var string url
	var string user
	var string password
	var word:{Normal Transacted} mode = Normal
	var string domain
	var string clientID
	has MessageHandler

Concept:NamedIfNoSchema MessageHandler is component
	var Schema schema
	var string subdomain = ""
	sub Feeder
	sub Mounter
	sub Process

Concept Client