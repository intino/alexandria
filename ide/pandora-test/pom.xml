            <version>[1.0.0, 2.0.0)</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>io.intino.pandora</groupId>
            <artifactId>pandora-slack</artifactId>
            <version>[1.0.0, 2.0.0)</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>io.intino.pandora</groupId>
